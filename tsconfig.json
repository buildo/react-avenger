  "include": ["src"],
  "compilerOptions": {
    "lib": [
      "es2015"
    ],
    "outDir": "lib",
    "target": "es5",
    "declaration": false,
    "allowJs": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
