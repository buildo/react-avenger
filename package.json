{
  "name": "react-avenger",
  "version": "8.0.0",
  "description": "react bindings for avenger",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "preversion": "npm run prettier-check && npm run lint && npm run typings-checker && npm test",
    "prepublish": "npm run build",
    "release-version": "smooth-release",
    "build": "rm -rf lib && mkdir lib && babel src -d lib --ignore test*.js && cp src/index.d.ts lib",
    "typings-checker": "typings-checker --allow-expect-error --project ./tsconfig.json $(find ./typings-checker -name \"*.ts*\")",
    "prettier-check": "prettier --list-different \"{src,typings-checker}/{**/*,*}.{js,jsx,ts,tsx}\"",
    "lint": "scriptoni lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buildo/react-avenger.git"
  },
  "author": "Giovanni Gonzaga <giovanni@buildo.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buildo/react-avenger/issues"
  },
  "homepage": "https://github.com/buildo/react-avenger#readme",
  "devDependencies": {
    "@types/enzyme": "^3.1.14",
    "@types/jest": "^23.3.9",
    "@types/react": "^16.0.40",
    "avenger": "^4.0.2",
    "babel-cli": "^6.22.2",
    "babel-jest": "^22.4.1",
    "babel-preset-buildo": "^0.1.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint-config-buildo": "^0.2.2",
    "eslint-config-prettier": "^2.9.0",
    "io-ts": "1.3.3",
    "jest": "^23.5.0",
    "prettier": "^1.11.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "scriptoni": "^0.10.1",
    "smooth-release": "^7.0.0",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.6",
    "typings-checker": "^2.0.0"
  },
  "peerDependencies": {
    "avenger": "^3.0.0",
    "react": ">=0.14.9 <15 || >=15.3.x"
  },
  "dependencies": {
    "buildo-state": "^0.8.0",
    "prop-types": "^15.6.0",
    "rxjs": "^5.4.2"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./jestSetup.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
